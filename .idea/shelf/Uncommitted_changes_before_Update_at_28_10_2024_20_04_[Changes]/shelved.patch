+  <script>
+    document.querySelector('form').addEventListener('submit', function(event) {
+      event.preventDefault();
+
+      let fictionScore = 0;
+      let childrenScore = 0;
+
+      const answers = document.querySelectorAll('input[type="radio"]:checked');
+      answers.forEach(answer => {
+        if (answer.value === 'fiction') {
+          fictionScore++;
+        } else if (answer.value === 'children') {
+          childrenScore++;
+        }
+      });
+
+      let genre;
+      if (fictionScore > childrenScore) {
+        genre = 'fiction';
+      } else {
+        genre = 'children';
+      }
+
+      if (genre === 'fiction') {
+        window.location.href = 'fiction_book.html';
+      } else if (genre === 'children') {
+        window.location.href = 'childrensbook.html';
+      }
+    });
+  </script>
